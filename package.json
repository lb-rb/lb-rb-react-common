{
  "name": "lb-rb-react-common-example",
  "description": "lb-rb-react-common example",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "homepage": "https://www.lb-rb.org/",
  "repository": {
    "type": "git",
    "url": "https://github.com/lb-rb/lb-rb-react-common.git"
  },
  "bugs": {
    "url": "https://github.com/lb-rb/lb-rb-react-common/issues"
  },
  "scripts": {
    "exec": "PATH=\"$PATH:./node_modules/.bin\" $@",
    "build": "npm run exec -- gatsby build",
    "develop": "npm run exec -- gatsby develop",
    "flow": "npm run exec flow",
    "format": "npm run exec -- prettier --config .prettierrc --write \"**/*.{js,jsx,json,md}\"",
    "format:diff": "npm run exec -- prettier --config .prettierrc --list-different \"**/*.{js,jsx,json,md}\"",
    "lint": "npm run exec -- eslint .",
    "lint:fix": "npm run exec -- eslint --fix .",
    "test": "npm run exec -- testcafe all tests/",
    "test-cli": "npm run exec -- testcafe -c 2 chromium:headless,firefox:headless -s takeOnFails=true all tests/",
    "browsers": "npm run exec -- testcafe -b",
    "reports": "export DATE_TIME=$(date +%FT%H%M%S) && npm run exec -- testcafe 'chromium:headless:emulation:width=4096;height=2160' --video reports/${DATE_TIME}/videos --screenshots path=reports/${DATE_TIME}/screenshots,takeOnFails=false all tests/",
    "ci": "npm run exec -- npm-run-all format:diff --parallel lint flow test-cli",
    "start": "npm run develop",
    "serve": "npm run exec -- gatsby serve",
    "clean": "npm run exec -- gatsby clean"
  },
  "engines": {
    "node": ">13.3.0",
    "npm": "^6.13.1"
  },
  "workspaces": [
    "packages/*",
    "."
  ],
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@emotion/styled-base": "^10.0.31",
    "@mdx-js/mdx": "^1.5.8",
    "@mdx-js/react": "^1.5.8",
    "bootstrap": "^4.5.0",
    "dom-helpers": "^5.1.4",
    "emotion": "^10.0.27",
    "formik": "^2.1.4",
    "gatsby": "^2.20.8",
    "gatsby-env-variables": "^2.0.0",
    "gatsby-image": "^2.3.1",
    "gatsby-plugin-emotion": "^4.3.5",
    "gatsby-plugin-glamor": "^2.2.1",
    "gatsby-plugin-google-analytics": "^2.2.2",
    "gatsby-plugin-mdx": "^1.1.3",
    "gatsby-plugin-mdx-frontmatter": "0.0.2",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-remove-trailing-slashes": "^2.3.11",
    "gatsby-plugin-sharp": "^2.5.3",
    "gatsby-plugin-sitemap": "^2.3.1",
    "gatsby-remark-autolink-headers": "^2.2.1",
    "gatsby-remark-code-titles": "^1.1.0",
    "gatsby-remark-copy-linked-files": "^2.2.1",
    "gatsby-remark-embedder": "^1.16.0",
    "gatsby-remark-graphviz": "^1.2.1",
    "gatsby-remark-images": "^3.2.1",
    "gatsby-remark-normalize-paths": "^1.0.0",
    "gatsby-remark-prismjs": "^3.4.1",
    "gatsby-remark-responsive-iframe": "^2.3.1",
    "gatsby-remark-smartypants": "^2.2.1",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-remark": "^2.7.1",
    "gatsby-transformer-sharp": "^2.4.3",
    "glamor": "^2.20.40",
    "iban": "0.0.14",
    "libphonenumber-js": "^1.7.54",
    "normalize.css": "^8.0.1",
    "prism-react-renderer": "^1.0.2",
    "prismjs": "^1.18.0",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-bootstrap-date-picker": "github:bmanturner/react-bootstrap-date-picker#feature/bs4",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-social-icons": "^4.1.0",
    "remark-abbr": "^1.4.0",
    "superagent": "^5.3.1",
    "superagent-use": "^0.1.0",
    "typeface-fira-sans": "^0.0.75",
    "typeface-open-sans": "^0.0.75",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "@babel/preset-flow": "^7.9.0",
    "eslint-config-fbjs-opensource": "^2.0.1",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-testcafe": "^0.2.1",
    "flow-bin": "^0.115.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "testcafe": "^1.8.3"
  }
}
